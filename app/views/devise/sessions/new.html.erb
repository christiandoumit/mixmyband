
<div class="container">

  <div class="justify-content-between align-items-start" style="width: 100%">
    <div class="contenant general all-left bottom-border-general can-grow aligned-started">
     <h2>Log in</h2>
    </div>
  </div>


<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="form-inputs">
    <%= f.input :email,
                required: false,
                autofocus: true,
                input_html: { autocomplete: "email" } %>
    <%= f.input :password,
                required: false,
                input_html: { autocomplete: "current-password" } %>
    <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %>
  </div>

 <div class="button-submit login">
      <%= f.button :submit, "LOGIN" %>
    </div>
<% end %>

<%= render "devise/shared/links" %>
