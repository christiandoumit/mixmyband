<% if user.type_of_user == 'eventer' %>
  <%=  link_to user_path(user) do %>
  <div class="contenant bottom-border-eventer">
    <h5 class="section-title eventer" style="color: #83B594; text-decoration: none"><%= user.eventer_name.downcase %></h5>
  </div>
  <div class="contenant-not-flex all-left centered p-1">
    <p class="section-light-text" style="color: #83B594; text-decoration: none">type of venue: <strong><%= user.venue_type.downcase %></strong></p>
    <p class="section-light-text pb-3" style="color: #83B594; text-decoration: none">type of crowd: <strong><%= user.crowd_type %></strong></p>
    <h6 class="section-paragraph" style="color: #83B594; text-decoration: none"><%= user.venue_address %></h6>
  </div>
  <% end %>
<% end %>
