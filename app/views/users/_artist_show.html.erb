<div>
  <h1><%= @user.artist_name %></h1>
  <h2><%= @user.artist_type %></h2>
  <p>Available for <%= User::TYPEOFEVENT.sample %></p>
  <p>$<%= @user.price %>/hour</p>
  <div>
    <button class="w3-button w3-round-large">Book me</button>
    <button class="w3-button w3-round-large">Send a message</button>
  </div>
  <img src="<%= @user.avatar%>" alt="avatar" style="border-radius: 50%">
</div>
