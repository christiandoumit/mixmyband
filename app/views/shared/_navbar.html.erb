<% if user_signed_in? %>
  <div class="navbar-mmb">
    <div class="mmb-container">
      <div class="collapse" id="navbarToggleExternalContent">
         <div class="p-4">
            <ul>
              <% if current_user.type_of_user == "artist"  %>
                <li><%= link_to 'Display Eventers Index', users_path %></li>
              <% else current_user.type_of_user == "eventer" %>
                <li><%= link_to 'Display Artists Index', users_path %>
              <% end %></li>
              <li><%= link_to('Log out', destroy_user_session_path, method: :delete) %></li>
            </ul>
          </div>
      </div>

      <div class="mmb-row">

        <div class='mmb-column pl-2'>
          <div>
            <nav class="navbar">
              <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarToggleExternalContent" aria-controls="navbarToggleExternalContent" aria-expanded="false" aria-label="Toggle navigation">
                <i class="fas fa-th fa-lg"></i>
              </button>
            </nav>
          </div>
          <div><%= image_tag 'mixmyband-logo.svg' %></div>
        </div>

        <div class="mmb-column">
          <div>
            <i class="far fa-heart fa-2x" style="color: white"></i>
          </div>
          <div>
            <i class="fas fa-user-circle fa-2x px-3" style="color: white"></i>
          </div>
        </div>

      </div>
    </div>
  </div>
<% else %>
  <h1>Nothing</h1>
<% end %>






